<div>
  <header><%= current_user.name%>の投稿</header>
    <ul>
        <% if !@musics == [] %>
          <li>
            <% @musics.each do |music| %>
              <p><img src="<%= music.img %>"></p>
              <p><%= music.artist %></p>
              <p><%= music.title %></p>
              <p><%= music.album %></p>
              <p><a href="<%= music.sample %>">サンプル</p>
              <p>コメント：<%= music.comment %></p>
              <a href="/delete/:id">削除</a>
              <a href="/edit/:id">編集</a>
            <% end %>
          </li>
        <% else %>
          <li>投稿がありません</li>
        <% end %>
      </ul>
</div>